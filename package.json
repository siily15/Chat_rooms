{
  "name": "c2",
  "version": "1.0.0",
  "description": "realtime chat",
  "main": "server.js",
  "scripts": {
    "test": "nodemon ",
    "dev": "nodemon -x 'npm run dev:sync' -w tailwind.config.js -w postcss.config.js -w bs.config.js -e js",
    "sync": "browser-sync start --server --files \"/*\"",
    "tailwind": "postcss tailwind.css --output output.css",
    "watch:dev": "nodemon -x npm run dev -w tailwind.config.js -w tailwind.css",
    "watch": "run-p watch:dev sync"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "moment": "^2.29.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "socket.io": "^4.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  }
}
